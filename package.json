{
  "name": "openai_chatbot",
  "version": "1.0.0",
  "description": "Chatbot using OpenAI's assistant API. Built in Express, TS and html",
  "main": "dist",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.7.0",
    "@types/bluebird": "^3.5.24",
    "axios": "^1.6.2",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.18.2",
    "express-flash": "0.0.2",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "magnific-popup": "^1.1.0",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "openai": "^4.20.1",
    "tslint": "^5.11.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/mocha": "^5.2.5",
    "@types/node": "^20.10.4",
    "awesome-typescript-loader": "^5.2.0",
    "concurrently": "^8.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint-parser": "^18.0.0"
  },
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc -p tsconfig.json",
    "dev": "concurrently \"npx tsc --watch --pretty false\" \"nodemon --quiet dist/index.js\"",
    "test": "tsc && ./node_modules/.bin/mocha dist/**/*.spec.js",
    "lint": "tslint 'src/**/*.ts' --fix",
    "clean": "rm -rf dist/ .logs/",
    "start": "node dist",
    "migrate": "npx prisma migrate dev --name init",
    "seed": "npx prisma db seed"
  },
  "author": "Mors Eques <draceques@gmail.com>"
}